{
  "name": "notifications-client",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/notifications/client/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "rootDir": ".",
        "outputPath": "dist/libs/notifications/client",
        "main": "libs/notifications/client/src/index.ts",
        "tsConfig": "libs/notifications/client/tsconfig.lib.json",
        "assets": [
          "libs/notifications/client/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/notifications/client/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/notifications/client"
      ],
      "options": {
        "jestConfig": "libs/notifications/client/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}