{
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "name": "notifications-types",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "rootDir": ".",
        "outputPath": "dist/libs/notifications/types",
        "main": "libs/notifications/types/src/index.ts",
        "tsConfig": "libs/notifications/types/tsconfig.lib.json",
        "assets": [
          "libs/notifications/types/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/notifications/types/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/libs/notifications/types"
      ],
      "options": {
        "jestConfig": "libs/notifications/types/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}